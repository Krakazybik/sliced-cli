{
  "name": "sliced-cli",
  "version": "0.0.1",
  "description": "Feature-sliced CLI tool",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "prestart": "npm run build",
    "build": "tsc --build",
    "start": "node .",
    "server:watch": "ts-node-dev --prefer-ts true -r tsconfig-paths/register --respawn --transpile-only src/index.ts"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "effector": "^22.1.2",
    "fs-extra": "^10.0.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
